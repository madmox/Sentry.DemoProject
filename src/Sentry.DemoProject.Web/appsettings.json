{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "Sentry": {
      "Dsn": "", // enter your DSN here
      "IncludeRequestPayload": true, // true generates errors when manipulating request body stream directly, see https://github.com/getsentry/sentry-dotnet/issues/175
      "SendDefaultPii": true,
      "MinimumBreadcrumbLevel": "Information",
      "MinimumEventLevel": "Warning",
      "AttachStackTrace": true
    }
  },
  "AllowedHosts": "*"
}
